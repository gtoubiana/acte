{
  "name": "acte",
  "version": "0.0.18",
  "description": "Une librairie JavaScript qui simplifie la recherche généalogique",
  "main": "dist/acte.min.js",
  "directories": {
    "test": "/test",
    "src": "/src"
  },
  "scripts": {
    "fromscratch": "npm install && bower install && npm run atom.install && gulp && gulp tests.coverage",
    "preupdate": "npm install -g npm && npm install -g npm-check-updates && npm install -g npm-check",
    "update": "ncu -m bower && ncu && npm-check -u",
    "prepull": "rm -rf ~/GitHub/acte/node_modules/ && rm -rf ~/GitHub/acte/bower_components/",
    "pull": "git pull origin && npm run fromscratch",
    "test": "gulp && gulp tests.coverage && gulp tests.karma",
    "tasks": "gulp tasks > ~/Github/acte/gulp/README.md",
    "atom.list": "apm list --installed --bare > ~/Github/acte/.atom.pkg",
    "atom.preinstall": "rm -rf ~/.atom/packages/",
    "atom.install": "apm install --packages-file ~/Github/acte/.atom.pkg",
    "delta": "gulp delta.scrap && gulp delta.csv && gulp delta.json && gulp delta.js && gulp delta.clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gtoubiana/acte.git"
  },
  "keywords": [
    "généalogie",
    "genealogy",
    "calendriers",
    "calendars",
    "acte",
    "javascript",
    "js",
    "script"
  ],
  "author": "Gilles Toubiana",
  "maintainers": [
    {
      "name": "gtoubiana",
      "email": "gilles.toubiana@yahoo.fr"
    }
